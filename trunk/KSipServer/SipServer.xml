<Setup>
	<Sip>
		<LocalIp>192.168.0.7</LocalIp>
		<UdpPort>5060</UdpPort>
		<UdpThreadCount>1</UdpThreadCount>
		<TcpPort>5060</TcpPort>
		<TcpThreadCount>1</TcpThreadCount>
		<TcpRecvTimeout>600</TcpRecvTimeout>
		<Realm>ksipserver</Realm>
		<!--<SendOptionsPeriod>30</SendOptionsPeriod>-->
		<TlsPort>5061</TlsPort>
		<TlsAcceptTimeout>10</TlsAcceptTimeout>
		<CertFile>c:\OpenProject\CppSipStack\trunk\KSipServer\SipServer.pem</CertFile>
		<MinRegisterTimeout>300</MinRegisterTimeout>
		<CallPickupId>**</CallPickupId>
	</Sip>
	<RtpRelay>
		<UseRtpRelay>true</UseRtpRelay>
		<BeginPort>10000</BeginPort>
		<EndPort>60000</EndPort>
	</RtpRelay>
	<Log>
		<Folder>c:\temp\sipserver</Folder>
		<Level Debug="true" Info="true" Network="true" Sql="true" />
		<MaxSize>10000000</MaxSize>
	</Log>
	<XmlFolder>
		<User>c:\OpenProject\CppSipStack\trunk\KSipServer\UserXml</User>
		<SipServer>c:\OpenProject\CppSipStack\trunk\KSipServer\SipServerXml</SipServer>
	</XmlFolder>
	<Cdr>
		<Folder>c:\temp\cdr</Folder>
	</Cdr>
	<Monitor>
	  <Port>6000</Port>
	  <ClientIpList>
	    <ClientIp>127.0.0.1</ClientIp>
	  </ClientIpList>
	</Monitor>
	<Security>
		<DenySipUserAgentList>
			<SipUserAgent>friendly-scanner</SipUserAgent>
			<SipUserAgent>sundayddr</SipUserAgent>
		</DenySipUserAgentList>
	</Security>
</Setup>
